<nav class="navbar navbar-default navbar-fixed-top" role="navigation">
    <div class="nav-wrapper">
        <ul class="navbar-right">
            <li class="active" id="login"><a href="sign_up">Sign up</a></li>
        </ul>
        <ul class="navbar-left">
            <li class="active" id="createeventbutton"><a href="events/new"><%= image_tag("createeventbutton.png", height: 50, width: 95, id: "createeventbutton") %></a></li>
            <li class="active" id="findeventbutton"><a href="events"><%= image_tag("findeventbutton.png", height: 50, width: 100, id: "findeventbutton") %></a></li>
            <li class="active" id="logo"><%= link_to image_tag("weact.jpg", height: 80, width: 180, id: "logo"), root_path %></li>
        </ul>
    </div>
